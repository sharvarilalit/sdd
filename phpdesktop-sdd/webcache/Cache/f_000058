    <script>
        $.ajaxSetup({
            headers: {
                'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')
            }
        });
        $('.dltBtn').click(function (e) {
            let form = $(this).closest('form');
            let dataId = $(this).data('id');
            e.preventDefault();
            swal({
                title: "Are you sure?",
                text: "Once deleted, you will not be able to recover this record!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        form.submit();
                        swal("Poof! Your record has been deleted!", {
                            icon: "success",
                        });
                    } else {
                        swal("Your record is safe!");
                    }
                });
        });
    </script>
    <script>
        $("#customSwitch1").on('click', function (e) {
            let form = $(this).closest('form');
            e.preventDefault();
            swal({
                title: "Are you sure?",
                text: "Are you sure want to approval!",
                icon: "warning",
                buttons: true,
                dangerMode: true,
            })
                .then((willDelete) => {
                    if (willDelete) {
                        form.submit();
                        swal("Poof! Approved has been successfully!", {
                            icon: "success",
                        });
                    } else {
                        swal("Your record is in pending!");
                    }
                });
        });
    </script>

    <script>
        $('#master_id').on('click', function (e) {
            if ($(this).is(':checked', true)) {
                $(".sub_chk").prop('checked', true);
            } else {
                $(".sub_chk").prop('checked', false);
            }
        });
        $('.bulk_action').on('change', function (e) {
            let allVals = [];
            if ($(this).val() == 2) {
                $(".sub_chk:checked").each(function () {
                    allVals.push($(this).attr('data-id'));
                });
                if (allVals.length <= 0) {
                    alert("Please select row.");
                } else {
                    let check = confirm("Are you sure you want to delete this row?");
                    if (check === true) {
                        let join_selected_values = allVals.join(",");
                        $.ajax({
                            url: 'http://127.0.0.1:52818/admin/member-bulk-status',
                            type: 'post',
                            headers: {'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')},
                            data: {ids: join_selected_values, action: $(this).val()},
                            success: function (data) {
                                $("#message").removeAttr('style')
                                if (data['success']) {
                                    $('#message').append('<div class="alert alert-success alert-dismissible fade show" id="alert" role="alert">' + data['success'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                } else if (data['error']) {
                                    $('#message').append('<div class="alert alert-danger alert-dismissible fade show" id="alert" role="alert">' + data['error'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                } else {
                                    $('#message').append('<div class="alert alert-danger alert-dismissible fade show" id="alert" role="alert">' + data['error'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                }
                            }
                            ,
                            error: function (data) {
                                alert(data.responseText);
                            }
                        })
                        ;
                    }
                }
            }

            if ($(this).val() == 0 || $(this).val() == 1) {
                $(".sub_chk:checked").each(function () {
                    allVals.push($(this).attr('data-id'));
                });
                if (allVals.length <= 0) {
                    alert("Please select row.");
                } else {
                    let check = confirm("Are you sure you want to update this row?");
                    if (check === true) {
                        let join_selected_values = allVals.join(",");
                        $.ajax({
                            url: 'http://127.0.0.1:52818/admin/member-status',
                            type: 'post',
                            headers: {'X-CSRF-TOKEN': $('meta[name="csrf-token"]').attr('content')},
                            data: {ids: join_selected_values, action: $(this).val()},
                            success: function (data) {
                                $("#message").removeAttr('style');
                                if (data['success']) {
                                    $('#message').append('<div class="alert alert-success alert-dismissible fade show" id="alert" role="alert">' + data['success'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                } else if (data['error']) {
                                    $('#message').append('<div class="alert alert-success alert-dismissible fade show" id="alert" role="alert">' + data['error'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                } else {
                                    $('#message').append('<div class="alert alert-success alert-dismissible fade show" id="alert" role="alert">' + data['error'] +
                                        '<button type="button" class="close" data-dismiss="alert" aria-label="Close">' +
                                        '</button></div> ');
                                    setTimeout(function () {
                                        location.reload();
                                    }, 2000);
                                }
                            }
                            ,
                            error: function (data) {
                                alert(data.responseText);
                            }
                        })
                        ;
                    }
                }
            }
        });
    </script>
